package-as: CrestXmuteHelper

enable-nolib-creation: no

ignore:
    - README.md
    - LICENSE
    - .git
    - .github
    - .gitignore
    - .pkgmeta

# Use curated changelog content for provider release notes
changelog-title: Crest Xmute Helper v@project-version@
manual-changelog: CHANGELOG_RELEASE.md

# WoWInterface-specific behavior: we'll use manual changelog and convert markdown
wowi-create-changelog: no
wowi-convert-changelog: yes